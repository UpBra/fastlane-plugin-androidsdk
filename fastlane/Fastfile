lane :test do
	avdmanager_create(
		name: 'TEST',
		package: 'system-images;android-32;google_apis;arm64-v8a',
		device: 'pixel_5',
		size: '4096M',
	)

	avdmanager_delete()

	avdmanager_execute(
		name: 'TEST',
		package: 'system-images;android-32;google_apis;arm64-v8a',
		device: 'pixel_5',
		size: '4096M',
		block: ->(name) {
			UI.message "Executing stuff with #{name} running..."
		}
	)
end
